/**coupons.wxss**/
.store-info {
  display: flex;
  justify-content: space-between;
  padding: 50rpx 30rpx;
}
.info-left {
  overflow: hidden;
}
.info-left >.h5-title {
  display: block;
  font-size: 28rpx;
  color: #353535;
  padding-bottom: 15rpx;
}
.info-left >.h5-text{
  display: block;
  font-size: 22rpx;
  color: #888888;
}
.distance{
  box-sizing: border-box;
  text-align: center;
  padding: 0 35rpx;
  align-self: center;
}
.distance image {
  display: inline-block;
  width: 33rpx;
  height: 42rpx;
}
.distance text {
  display: block;
  font-size: 28rpx;
  color: #595757;
}
.info-alert{
  padding: 30rpx;
  font-size: 22rpx;
  color: #595757;
  border: 1px solid #eeeff3;
  box-sizing: border-box;
}
.info-alert text{
  display: block;
}
.info-option{
  padding: 30rpx;
  overflow: hidden;
  border-bottom: 1px solid #eeeff3;
}
.info-option >text{
  float: left;
  position: relative;
  padding-left: 30rpx;
  font-size: 22rpx;
  color: #353535;
  margin-right: 30rpx;
}
.info-option >text:before{
  content: '';
  position: absolute;
  left: 0;
  top: 4rpx;
  width: 23rpx;
  height: 23rpx;
  background-image: url('http://boc.huangquemaishou.com/images/icon-check.png');
  background-size: 100% auto;
  background-repeat: no-repeat;
}
.info-tips{
  text-align: center;
  padding: 38rpx 0;
  display: flex;
  justify-content: center;
}
.info-tips >image {
  width: 52rpx;
  height: 40rpx;
  align-self: center;
}
.info-tips >text{
  align-self: center;
  font-size: 34rpx;
  margin-left: 16rpx;
}
.info-title {
  color: #353535;
  font-size: 34rpx;
  padding: 30rpx;
  border-top: 1px solid #eeeff3;
  border-bottom: 1px solid #eeeff3;
  font-weight: 400;
}
.info-card {
  padding: 38rpx 30rpx 0 30rpx;
}
.info-card image{
  width: 100%;
}
.coupons-list{
  overflow: hidden;
  padding: 0 30rpx;
}
.coupons-list >.h5-title {
  display: block;
  padding: 30rpx 0;
  text-align: center;
  font-size: 22rpx;
  color: #888888;
  font-weight: normal;
  letter-spacing: 1px;
}
.coupons-list .coupons {
    display: flex;
    justify-content: space-between;
    width: 100%;
    background-color: #fff;
    border-radius: 10px;
    margin-top: 18rpx;
    box-shadow: 0px 0px 16px rgba(0,0,0,0.08);
    overflow: hidden;
    padding: 15rpx 0;
}
.coupons .coup-left {
  margin-left: 15rpx;
  display: flex;
}
.coup-left .money {
  font-size: 34rpx;
  color: #353535;
  align-self: center;
}
.money >text {
  font-size: 68rpx;
  color: #fb726f;
}
.coupons-det {
  margin-left: 20rpx;
  align-self: center;
}
.coupons-det .h5-title {
  display: block;
  font-size: 26rpx;
  color: #353535;
}
.coupons-det .h5-text {
  display: block;
  font-size: 22rpx;
  color: #888888;
}
.coupons .coup-right {
  border-left: 1px solid #eeeff3;
  padding: 40rpx 28rpx;
}
.coupons .coup-right .to-receive {
  display: inline-block;
  overflow: hidden;
  padding: 12rpx 30rpx;
  border-radius: 50px;
  font-size: 22rpx;
  color: #ffffff;
  background-color: #fb726f;
}
.tags{
  position: absolute;
  top: 1rpx;
  right: 30rpx;
  z-index: 99;
}
.tags text{
  float: left;
  line-height: 1.3;
  padding-top: 5rpx;
  margin-top: -3rpx;
  width: 52rpx;
  height: 74rpx;
  text-align: center;
  font-size: 20rpx;
  color: #ffffff;
  background-size: 100% 100%;
  background-repeat: no-repeat;
  margin-left: 10rpx;
}
 .tags >text:first-child {
    background-image: url(data:image/png;base64,);
}
.tags >text:nth-of-type(2) {
    background-image: url(data:image/png;base64,);
} 
